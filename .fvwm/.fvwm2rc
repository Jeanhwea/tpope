PipeRead "$HOME/bin/hostinfo --fvwm-keys"
Key 0       A   CM      Exec exec favorite-app
Key 1       A   CM      Exec exec sensible-browser http://www.google.com/
Key 2       A   CM      Exec [ -x /usr/sbin/automount ] && sudo /usr/bin/killall -USR1 automount; [ -x /sbin/cardctl ] && sudo /sbin/cardctl eject
Key 3       A   CM      FuncFvwmExec gaim "exec gaim"
Key 4       A   CM      FuncFvwmIrcScreenSshXterm
Key 6       A   CM      Exec exec xpenguins --nice .8 3
Key 7       A   CM      Module FvwmConsole
Key 9       A   CM      Exec exec aumix
Key Space   A   CM      FuncFvwmScreenXterm

Key 1       A   SCM     Exec exec sensible-browser
Key 2       A   SCM     Exec sleep 1; xscreensaver-command -lock || exec xlock
Key 3       A   SCM     Exec exec gaim -a
Key 4       A   SCM     FuncFvwmIrcScreenXterm
Key 6       A   SCM     Exec exec xpenguins -t "Big Penguins" --nice .8 3
Key 7       A   SCM     Exec exec pkill -U $[USER] xpenguins
Key 9       A   SCM     Exec tvtime || exec xawtv
Key Space   A   SCM     FuncFvwmXterm
Key G       A   SCM     Exec exec gaim -a
Key W       A   SCM     Exec exec sensible-browser
Key Z       A   SCM     Exec tvtime || exec xawtv

Key Home    A   4       Exec exec aumix -w +5
