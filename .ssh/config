# ~/.ssh/config
# vim:set ft=sshconfig:

Host michael.*
	HostKeyAlias michael

Host buster.*
	HostKeyAlias buster

Host tobias.*
	HostKeyAlias tobias

Host lindsay.*
	HostKeyAlias lindsay

Host gob.*
	HostKeyAlias gob

Host george.*
	HostKeyAlias george

Host lucille.*
	HostKeyAlias lucille

Host maeby.*
	HostKeyAlias maeby

Host oscar.*
	HostKeyAlias oscar

Host *.tpope.us michael* gob* lindsay* tobias* buster* lucille* maeby* oscar* george* jwxkl81-1061* bjdxx51-594* *-netmon*
	ForwardX11 yes

Host tpope.info
	Port 4567
	ForwardX11 no

Host trp
	ProxyCommand none
	Hostname tpope.info
	Port 4567
	HostKeyAlias michael
	CheckHostIP no
	ForwardX11 no
	ServerAliveInterval 240

Host jmwa
	ProxyCommand none
	Hostname tpope.info
	Port 4445
	HostKeyAlias george
	CheckHostIP no
	ForwardX11 no
	ServerAliveInterval 240

Host *.p
	ProxyCommand connect -H www.tpope.net `echo %h|sed -e s/\.p$//` %p

Host *.proxy
	ProxyCommand connect -H www.tpope.net `echo %h|sed -e s/\.proxy$//` %p

Host jmwaller
	#IdentityFile /dev/null
	Hostname JWXKL81-1061
	ProxyCommand ssh -ax jmwa nc -q 1 JWXKL81-1061.jmwaller.com %p 2>/dev/null
	#Port 19021

Host jwxkl81-1061 jwxkl81-1061.*
	#IdentityFile /dev/null
	Hostname JWXKL81-1061

Host bjdxx51-594 bjdxx51-594.*
        Hostname BJDXX51-594

Host *.jmwaller.com *.j
	ProxyCommand ssh -ax jmwa nc -q 1 `echo %h|sed -e s/\.j.*$//` %p 2>/dev/null

Host *.tpope.us *.t
	ProxyCommand ssh -ax trp nc -q 1 `echo %h|sed -e s/\.t.*$//` %p 2>/dev/null

Host tpope.us cunn.iling.us
	CheckHostIP no
	DynamicForward 1080

Host *.* localhost
	ProxyCommand none

Host *
	User tpope
	ForwardAgent yes
	Ciphers blowfish-cbc,aes128-cbc,3des-cbc,cast128-cbc,arcfour,aes192-cbc,aes256-cbc
	ProxyCommand sh -c '$HOME/bin/hostinfo --tunnel %h %p'
