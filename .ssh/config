# ~/.ssh/config
# $Id$
# vim: ft=sshconfig

Host gob.* maggie.*
	HostKeyAlias gob

Host barry.* bart.*
	HostKeyAlias barry

Host lucille.* lisa.*
	HostKeyAlias lucille

Host michael.* marge.*
	HostKeyAlias michael

Host maeby.* homer.*
	HostKeyAlias maeby

Host lindsay.* abe.*
	HostKeyAlias lindsay

Host tobias.* mona.*
	HostKeyAlias tobias

Host steve.* snowball.*
	HostKeyAlias steve

Host oscar.* clancy.*
	HostKeyAlias oscar

Host buster.* sarah.*
	HostKeyAlias buster

Host lupe.* ralph.*
	HostKeyAlias lupe

Host *.tpope.us gob* barry* lucille* michael* maeby* lindsay* tobias* oscar* buster* lupe* maggie* tpope-486* tpope-1084* jwxkl81-1061* netmon1* images*
	ForwardX11 yes

Host *iling.us rebelongto.us
	Port 4567
	ForwardX11 no

Host trp
	ProxyCommand none
	Hostname tpope.info
	Port 4567
	HostKeyAlias gob
	CheckHostIP no
	ForwardX11 no
	ServerAliveInterval 240

Host jmwa
	ProxyCommand none
	Hostname tpope.info
	Port 4445
	HostKeyAlias tpope-486
	CheckHostIP no
	ForwardX11 no
	ServerAliveInterval 240

Host *.p
	ProxyCommand connect -H www.tpope.net `echo %h|sed -e s/\.p$//` %p

Host *.proxy
	ProxyCommand connect -H www.tpope.net `echo %h|sed -e s/\.proxy$//` %p

Host jmwaller
	IdentityFile ""
	Hostname JWXKL81-1061
	ProxyCommand ssh -ax jmwa nc -q 1 JWXKL81-1061.jmwaller.com %p 2>/dev/null
	#Port 19021

Host jwxkl81-1061 jwxkl81-1061.*
	IdentityFile ""
	Hostname JWXKL81-1061

Host *.jmwaller.com *.j
	ProxyCommand ssh -ax jmwa nc -q 1 `echo %h|sed -e s/\.j.*$//` %p 2>/dev/null

Host *.tpope.us *.t
	ProxyCommand ssh -ax trp nc -q 1 `echo %h|sed -e s/\.t.*$//` %p 2>/dev/null

Host sexygeek.us cunn.iling.us
	CheckHostIP no
	DynamicForward 1080

Host *.* localhost
	ProxyCommand none

Host *
	User tpope
	ForwardAgent yes
	Ciphers blowfish-cbc,aes128-cbc,3des-cbc,cast128-cbc,arcfour,aes192-cbc,aes256-cbc
	ProxyCommand hostinfo --tunnel %h %p
