# ~/.ssh/config
# $Id$
# vim: ft=sshconfig

Host maggie.*
	HostKeyAlias maggie

Host bart.*
	HostKeyAlias bart

Host lisa.*
	HostKeyAlias lisa

Host marge.*
	HostKeyAlias marge

Host homer.*
	HostKeyAlias homer

Host abe.*
	HostKeyAlias abe

Host snowball.*
	HostKeyAlias snowball

Host mona.*
	HostKeyAlias mona

Host clancy.*
	HostKeyAlias clancy

Host sarah.*
	HostKeyAlias sarah

Host ralph.*
	HostKeyAlias ralph

#Host grex* *cyberspace.org
#	ForwardX11 no

Host *.simpson maggie* bart* lisa* marge* homer* abe* mona* clancy* sarah* ralph* barney*
	ForwardX11 yes

Host *iling.us
	Port 4567

Host rebelongto.us
	Port 8765
	HostKeyAlias bart
	CheckHostIP no

Host *.proxy
	ProxyCommand connect -H www.sexygeek.org `echo %h|sed -e s/\.proxy$//` %p

Host *.iling
	ProxyCommand ssh -ax iling.us nc -q 2 `echo %h|sed -e s/\.iling$//` %p

Host sexygeek.us cunn.iling.us
	CheckHostIP no
	DynamicForward 1080

Host *
	User tpope
	ForwardAgent yes
	Ciphers blowfish-cbc,aes128-cbc,3des-cbc,cast128-cbc,arcfour,aes192-cbc,aes256-cbc
#	HostKeyAlgorithms ssh-dss,ssh-rsa
